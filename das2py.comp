# in: [ root name d2f das2f das2j ]
# out: [ hello.py world.py helloworld.py top.py ]

set -x 
echo das2py.comp

input_root=/dev/fd/3
input_name=/dev/fd/4
input_d2f=/dev/fd/5
input_das2f=/dev/fd/6
input_das2j=/dev/fd/7

read das2j <$input_das2j

echo $das2j

$das2j 3<$input_root 4<$input_name 5<$input_d2f 6<$input_das2f 7>&1 &
pid=$!
